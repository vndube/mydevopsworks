BROKER SCHEMA com.icici.xpress_connect.ecollection.mh.ci.intimation

CREATE FILTER MODULE IsCustomerAsync
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN

		IF CLIENT_API_MODE = CLIENT_API_MODE_SYNC THEN
			RETURN FALSE;
		ELSEIF CLIENT_API_MODE = CLIENT_API_MODE_ASYNC THEN
			RETURN TRUE;
		END IF;
	END;

END MODULE;
